{
  "name": "typescript-rest-api",
  "version": "1.0.0",
  "description": "Learning TypeScript",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "ts-node": "ts-node ./src",
    "dev:start": "npm-run-all build start",
    "dev:ts-node": "nodemon --watch src -e ts --exec npm run ts-node",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "start": "node .",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "keywords": [
    "API Rest",
    "Docker",
    "TypeScript"
  ],
  "author": "Christian Barrios",
  "license": "MIT",
  "dependencies": {
    "@types/supertest": "^2.0.8",
    "compression": "^1.7.4",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@types/compression": "^1.0.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.17",
    "@types/mongoose": "^5.5.12",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.2",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
